	Keywords = {associative memory; ERP},
	Mesh = {Adult; Association Learning; Discrimination Learning; Evoked Potentials; Female; Frontal Lobe; Humans; Male; Mental Recall; Parietal Lobe; Recognition (Psychology); Reference Values; Verbal Learning},
	Number = {9},
	Pages = {1595--1605},
	Pmid = {16989559},
	Pst = {ppublish},
	Rating = {5},
	Title = {Contribution of familiarity and recollection to associative recognition memory: insights from event-related potentials},
	Volume = {18},
	Year = {2006},
