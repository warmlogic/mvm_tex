	Keywords = {fMRI; machine learning; spatial memory; hippocampus; MTL},
	Pmid = {23016766},
	Pst = {aheadofprint},
	Title = {{Disentangling Spatial Perception and Spatial Memory in the Hippocampus: A Univariate and Multivariate Pattern Analysis fMRI Study}},
	Year = {2012},
