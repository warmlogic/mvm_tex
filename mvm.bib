	Journal-Full = {International journal of psychophysiology : official journal of the International Organization of Psychophysiology},
	Keywords = {spacing effect; ERP; EEG; oscillations; induced; repetition suppression},
	Mesh = {Adolescent; Adult; Analysis of Variance; Brain Mapping; Electroencephalography; Evoked Potentials, Visual; Female; Humans; Male; Neuropsychological Tests; Reaction Time; Recognition (Psychology); Semantics; Spectrum Analysis},
	Number = {3},
