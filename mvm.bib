	Keywords = {hippocampus; MTL; spatial memory; fMRI},
	Mesh = {Adult; Brain Mapping; Dominance, Cerebral; Face; Female; Functional Laterality; Humans; Magnetic Resonance Imaging; Male; Neuropsychological Tests; Parahippocampal Gyrus; Pattern Recognition, Visual; Photic Stimulation; Reaction Time; Recognition (Psychology); Temporal Lobe; Visual Cortex; Visual Pathways; Visual Perception; Young Adult},
	Number = {2},
	Pages = {294--303},
	Pmc = {PMC2803731},
	Pmid = {19457939},
	Pst = {ppublish},
	Title = {How reliable are visual context effects in the parahippocampal place area?},
	Volume = {20},
	Year = {2010},
