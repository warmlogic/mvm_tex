	Keywords = {synchronization; MEG; fMRI; recognition memory; perception},
	Mesh = {Adult; Brain Mapping; Electroencephalography Phase Synchronization; Female; Hippocampus; Humans; Magnetic Resonance Imaging; Magnetoencephalography; Male; Prefrontal Cortex; Reaction Time; Recognition (Psychology); Time Factors; Visual Perception; Young Adult},
