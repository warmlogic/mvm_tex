	Keywords = {machine learning; hippocampus; fMRI},
	Pages = {290},
	Pmc = {PMC3474998},
	Pmid = {23087638},
	Pst = {ppublish},
	Title = {Multi-voxel pattern analysis in human hippocampal subfields},
	Volume = {6},
