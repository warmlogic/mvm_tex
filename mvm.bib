	Author = {Spaniol, Julia and Davidson, Patrick S R and Kim, Alice S N and Han, Hua and Moscovitch, Morris and Grady, Cheryl L},
	Date-Added = {2011-11-18 22:27:01 +0000},
	Date-Modified = {2011-11-18 22:27:29 +0000},
	Doi = {10.1016/j.neuropsychologia.2009.02.028},
	Journal = {Neuropsychologia},
	Journal-Full = {Neuropsychologia},
	Keywords = {fMRI; metaanalysis; encoding; recognition memory},
	Mesh = {Brain; Brain Mapping; Evoked Potentials; Humans; Image Processing, Computer-Assisted; Likelihood Functions; Magnetic Resonance Imaging; Mental Recall; Neuropsychological Tests; Oxygen; Recognition (Psychology)},
	Number = {8-9},
	Pages = {1765--1779},
	Pmid = {19428409},
	Pst = {ppublish},
	Title = {Event-related fMRI studies of episodic encoding and retrieval: meta-analyses using activation likelihood estimation},
	Volume = {47},
