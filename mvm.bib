	Journal = {Brain Research. Cognitive Brain Research},
	Journal-Full = {Brain research. Cognitive brain research},
	Keywords = {episodic memory; ERP; source memory; LPN},
	Mesh = {Adult; Evoked Potentials, Visual; Female; Humans; Male; Mastoid; Memory; Nose; Parietal Lobe; Pattern Recognition, Visual; Reaction Time; Visual Cortex},
	Number = {2--3},
