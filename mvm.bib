	Keywords = {models; RK},
	Mesh = {Cognition; Discrimination (Psychology); Humans; Judgment; Memory; Psychology; Retention (Psychology)},
	Number = {5},
	Pages = {906--926},
	Pmid = {18926982},
	Pst = {ppublish},
	Title = {Evaluating models of remember-know judgments: complexity, mimicry, and discriminability},
	Volume = {15},
