	Keywords = {SME; source memory; attention; fMRI; stimuli},
	Mesh = {Adolescent; Adult; Attention; Color Perception; Female; Functional Laterality; Hippocampus; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Memory; Neuropsychological Tests; Pattern Recognition, Visual; Photic Stimulation; Psychomotor Performance; Space Perception; Young Adult},
	Number = {25},
