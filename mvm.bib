	Keywords = {class},
	Mesh = {Attention; Brain; Cognition; Functional Laterality; Humans; Learning; Linguistics; Mathematics; Memory, Short-Term; Psychological Theory; Schizophrenia; Visual Fields},
	Number = {1},
	Pages = {87--114; discussion 114--185},
	Pmid = {11515286},
	Title = {The magical number 4 in short-term memory: a reconsideration of mental storage capacity},
	Volume = {24},
