	Journal = {J Neurosci},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {PFC; SME; binding; recognition memory; encoding},
	Mesh = {Adolescent; Adult; Female; Humans; Magnetic Resonance Imaging; Male; Memory; Photic Stimulation; Prefrontal Cortex; Reaction Time; Word Association Tests},
	Number = {20},
