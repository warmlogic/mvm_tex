	Keywords = {SME; oscillations; episodic memory; attention; theta; gamma; synchronization; EEG; scalp},
	Mesh = {Adolescent; Adult; Attention; Electroencephalography; Female; Humans; Male; Nerve Net; Photic Stimulation; Predictive Value of Tests},
