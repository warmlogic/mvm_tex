	Mesh = {Adolescent; Adult; Brain Mapping; Female; Humans; Magnetic Resonance Imaging; Male; Mental Recall; Models, Neurological; Parahippocampal Gyrus; Parietal Lobe; Prefrontal Cortex; Reaction Time; Recognition (Psychology)},
	Number = {17},
