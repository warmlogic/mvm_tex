@article{CurrDien2003,
	Abstract = {Distinct event-related potential effects have been related to familiarity and recollection processes underlying recognition memory. Familiarity has been conceptualized as similar either to perceptual priming mechanisms supporting implicit memory or to amodal global-matching processes that should show little sensitivity to perceptual variables. The present experiment manipulated the study modality of words (auditory, visual) that were visually tested for recognition memory. The mid-frontal (300-500 ms) old/new effect often attributed to familiarity was not affected by study modality, so it appears related to an amodal familiarity process. An earlier (176-260 ms) fronto-polar old/new effect was perceptually specific in that it was observed only following visual study. The parietal old/new effect (400-800 ms), often attributed to recollection, was similar following both visual and auditory study. Temporal-spatial PCA clarified the separability of these effects.},
	Author = {Curran, Tim and Dien, Joseph},
	Date-Added = {2010-08-26 12:16:07 -0600},
	Date-Modified = {2010-09-23 12:51:50 -0600},
