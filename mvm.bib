	Keywords = {spacing effect; neuron; Animals},
	Mesh = {Animals; Cell Division; Cell Survival; Dentate Gyrus; Hippocampus; Male; Maze Learning; Memory; Neurons; Rats; Rats, Sprague-Dawley; Retention (Psychology); Time Factors},
	Number = {5},
	Pages = {368--375},
	Pmc = {PMC1876761},
	Pmid = {17522028},
