	Journal-Full = {Journal of cognitive neuroscience},
	Keywords = {fMRI; confidence responses; response monitoring},
	Mesh = {Adult; Behavior; Cerebrovascular Circulation; Female; Functional Laterality; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Memory; Prefrontal Cortex; Recognition (Psychology)},
	Number = {6},
