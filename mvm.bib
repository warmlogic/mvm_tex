	Keywords = {recognition memory; MTL; patients; Humans; Animals},
	Mesh = {Amnesia; Anterior Thalamic Nuclei; Fornix, Brain; Hippocampus; Humans; Memory, Short-Term; Mental Recall; Midline Thalamic Nuclei; Models, Neurological; Neural Pathways; Recognition (Psychology); Temporal Lobe; Thalamus},
	Number = {3},
	Pages = {425--444; discussion 444--489},
	Pmid = {11301518},
