	Mesh = {Adult; Algorithms; Artifacts; Color; Data Interpretation, Statistical; Evoked Potentials; Female; Humans; Language; Learning; Magnetic Resonance Imaging; Male; Memory; Psychomotor Performance; Reading; Theta Rhythm},
	Number = {3},
