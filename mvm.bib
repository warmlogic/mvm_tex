	Keywords = {oscillations; methods; connectivity; synchronization; gamma; EEG; scalp; faces},
	Mesh = {Adult; Analysis of Variance; Attention; Automatic Data Processing; Brain Mapping; Cerebral Cortex; Discrimination Learning; Electroencephalography; Evoked Potentials, Visual; Facial Expression; Female; Generalization, Stimulus; Humans; Male; Pattern Recognition, Visual; Photic Stimulation; Reaction Time; Recognition (Psychology); Spectrum Analysis; Time Factors},
