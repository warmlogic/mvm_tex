	Keywords = {spacing effect; applied; TBI},
	Mesh = {Adolescent; Adult; Brain Injuries; Female; Humans; Male; Memory; Middle Aged; Models, Psychological; Pattern Recognition, Visual; Psychological Tests; Reaction Time; Retention (Psychology); Semantics; Task Performance and Analysis; Time Factors},
