	Doi = {10.1523/JNEUROSCI.1948-06.2006},
	Journal = {J Neurosci},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {EEG; oscillations; gamma; recognition memory; encoding; theta},
	Mesh = {Adult; Brain; Brain Mapping; Female; Humans; Magnetoencephalography; Male; Memory; Mental Recall; Photic Stimulation; Recognition (Psychology); Visual Perception},
	Number = {28},
	Pages = {7523--7531},
	Pmid = {16837600},
