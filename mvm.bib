	Keywords = {MTL; contextual reinstatement; hippocampus; binding; recognition memory; recollection; recall; fMRI},
	Number = {1},
	Pages = {808--817},
	Pmid = {22200724},
	Pst = {ppublish},
	Title = {Putting {Humpty} together and pulling him apart: accessing and unbinding the hippocampal item-context engram},
	Volume = {60},
	Year = {2012},
	Bdsk-File-1 = {}}

@article{YooEtal2012,
	Abstract = {The rate of learning or memory formation varies over time for any individual, partly due to moment-to-moment fluctuation of brain state. Functional neuroimaging has revealed the neural correlates of learning and memory, but here we asked if neuroimaging can causally enhance human learning by detection of brain states that reveal when a person is prepared or not prepared to learn. The parahippocampal cortex (PHC) is essential for memory formation for scenes. Here, activation in PHC was monitored in real-time, and scene presentations were triggered when participants entered "good" or "bad" brain states for learning of novel scenes. Subsequent recognition memory was more accurate for scenes presented in "good" than "bad" brain states. These findings show that neuroimaging can identify in real-time brain states that enhance or depress learning and memory formation, and knowledge about such brain states may be useful for accelerating education and training. Further, the use of functional neuroimaging as a causal, rather than correlative, tool to study the human brain may open new insights into the neural basis of human cognition.},
	Author = {Yoo, Julie J and Hinds, Oliver and Ofen, Noa and Thompson, Todd W and Whitfield-Gabrieli, Susan and Triantafyllou, Christina and Gabrieli, John D E},
	Date-Added = {2012-04-27 22:45:24 +0000},
	Date-Modified = {2013-07-16 18:13:49 +0000},
