	Journal = {Journal of Neuroscience},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {associative memory; recall},
	Mesh = {Adult; Association Learning; Brain Mapping; Cerebral Cortex; Discrimination (Psychology); Female; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Mental Recall; Recognition (Psychology)},
	Number = {36},
	Pages = {9162--9172},
	Pmid = {16957073},
