	Journal = {NeuroImage},
	Journal-Full = {NeuroImage},
	Keywords = {machine learning; ERP; EEG},
	Mesh = {Adolescent; Adult; Algorithms; Area Under Curve; Artificial Intelligence; Automobiles; Data Interpretation, Statistical; Electroencephalography; Evoked Potentials; Face; Female; Humans; Individuality; Linear Models; Male; Perception; Photic Stimulation; Principal Component Analysis; Psychomotor Performance; ROC Curve; Young Adult},
	Number = {4},
