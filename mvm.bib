	Keywords = {class; consolidation},
	Number = {32},
	Pages = {10087--10093},
	Pmid = {19675242},
	Rating = {0},
	Read = {Yes},
	Title = {Shift from hippocampal to neocortical centered retrieval network with consolidation},
	Volume = {29},
