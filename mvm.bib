	Journal-Full = {Neuropsychologia},
	Keywords = {parietal; fMRI; episodic memory; metaanalysis},
	Mesh = {Attention; Brain Mapping; Cognition; Cues; Decision Making; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Memory; Mental Recall; Models, Neurological; Neural Pathways; Parietal Lobe; Pattern Recognition, Visual; Psychomotor Performance; Reaction Time; Recognition (Psychology); Temporal Lobe; Verbal Behavior},
	Number = {7},
	Pages = {1828--1851},
	Pmid = {18471837},
	Pst = {ppublish},
	Title = {Top-down and bottom-up attention to memory: a hypothesis (AtoM) on the role of the posterior parietal cortex in memory retrieval},
