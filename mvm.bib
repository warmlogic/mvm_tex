	Keywords = {MTL; source memory; recognition memory; models; encoding},
	Mesh = {Animals; Brain Mapping; Humans; Memory; Models, Psychological; Neural Pathways; Neuropsychological Tests; ROC Curve; Recognition (Psychology); Temporal Lobe},
	Number = {11},
	Pages = {1206--1216},
	Pmid = {20824723},
	Pst = {ppublish},
	Rating = {5},
	Read = {1},
	Title = {Hierarchical relational binding in the medial temporal lobe: the strong get stronger},
	Volume = {20},
