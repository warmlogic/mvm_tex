@article{XueEtal2010,
	Abstract = {Repeated study improves memory, but the underlying neural mechanisms of this improvement are not well understood. Using functional magnetic resonance imaging and representational similarity analysis of brain activity, we found that, compared with forgotten items, subsequently remembered faces and words showed greater similarity in neural activation across multiple study in many brain regions, including (but not limited to) the regions whose mean activities were correlated with subsequent memory. This result addresses a longstanding debate in the study of memory by showing that successful episodic memory encoding occurs when the same neural representations are more precisely reactivated across study episodes, rather than when patterns of activation are more variable across time.},
	Annote = {I could imagine a similar experimental manipulation to their Experiment 3 that uses levels of processing judgments (maybe 4 judgments, 2 semantic 2 non-semantic) and compares the reinstatement and encoding variability hypotheses tested in this paper. Words are studied and given a LOP judgment (e.g., semantic), then on repeated study trials (three repeats, like in this paper) the words are either given the same LOP judgment (semantic; reinstatement) or a different one (non-semantic or the unused semantic; encoding variability). Then we could analyze the similarity of neural activity patterns for the encoding variability case (different judgments) vs the reinstatement case (same judgments) based on subsequent recall and recognition (with six-point confidence old/new). So, this would add an actual manipulation of the two competing hypotheses.},
	Author = {Xue, Gui and Dong, Qi and Chen, Chuansheng and Lu, Zhonglin and Mumford, Jeanette A and Poldrack, Russell A},
	Date-Added = {2012-10-27 18:33:05 +0000},
	Date-Modified = {2013-07-16 18:08:54 +0000},
	Doi = {10.1126/science.1193125},
	Journal = {Science},
	Journal-Full = {Science (New York, N.Y.)},
	Keywords = {fMRI; recognition memory; SME; encoding; pattern similarity},
	Mesh = {Adolescent; Adult; Brain; Brain Mapping; Face; Female; Frontal Lobe; Humans; Learning; Magnetic Resonance Imaging; Male; Memory; Mental Recall; Parietal Lobe; Recognition (Psychology); Temporal Lobe; Visual Cortex; Young Adult},
