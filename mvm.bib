	Keywords = {machine learning; fMRI; MTL; recognition memory; SME; learning},
	Mesh = {Adult; Brain Mapping; Female; Humans; Image Interpretation, Computer-Assisted; Learning; Magnetic Resonance Imaging; Male; Memory; Parahippocampal Gyrus; Recognition (Psychology)},
	Number = {1},
	Pages = {846--852},
	Pmid = {21821136},
	Pst = {ppublish},
	Read = {1},
	Title = {When the brain is prepared to learn: enhancing human learning using real-time {fMRI}},
	Volume = {59},
