	Journal-Full = {Brain research},
	Keywords = {oscillations; connectivity; STM; theta; alpha},
	Mesh = {Adolescent; Adult; Alpha Rhythm; Biological Clocks; Brain Mapping; Cerebral Cortex; Electroencephalography; Evoked Potentials; Female; Humans; Male; Memory, Short-Term; Nerve Net; Neural Pathways; Neuropsychological Tests; Photic Stimulation; Sensory Gating; Theta Rhythm; Young Adult},
	Pages = {126--132},
