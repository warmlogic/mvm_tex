	Doi = {10.1098/rstb.1999.0481},
	Journal = {Philos Trans R Soc Lond B Biol Sci},
	Journal-Full = {Philosophical transactions of the Royal Society of London. Series B, Biological sciences},
	Keywords = {SME; ERP; fMRI; PET; Dm; review},
	Mesh = {Brain; Evoked Potentials; Humans; Magnetic Resonance Imaging; Memory; Mental Recall; Tomography, Emission-Computed},
	Number = {1387},
