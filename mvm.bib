	Keywords = {machine learning; encoding; fMRI; SME; PFC},
	Mesh = {Adolescent; Adult; Brain; Brain Mapping; Female; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Male; Memory, Episodic; Neuroimaging; Neuropsychological Tests},
	Number = {4},
	Pages = {458--469},
