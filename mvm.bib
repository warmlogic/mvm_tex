	Keywords = {methods; oscillations; connectivity},
	Mesh = {Algorithms; Electrophysiology; Fourier Analysis; Humans; Mathematical Computing; Models, Neurological; Neurology; Neurons; Signal Processing, Computer-Assisted; Software; Time Factors},
	Number = {2},
	Pages = {321--332},
