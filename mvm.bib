	Mesh = {Actins; Animals; Dendritic Spines; Learning; Long-Term Potentiation; Male; Mice; Mice, Inbred C57BL; Polymerization; Rats; Rats, Sprague-Dawley; Synapses; Synaptic Transmission; Theta Rhythm; Time Factors},
	Number = {13},
	Pages = {5121--5126},
