	Keywords = {fMRI; SME; encoding; levels of processing; PFC; MTL},
	Mesh = {Adult; Brain Mapping; Cerebral Cortex; Female; Functional Laterality; Humans; Language Tests; Magnetic Resonance Imaging; Male; Mental Processes; Predictive Value of Tests; Recognition (Psychology); Reference Values},
	Number = {4-5},
	Pages = {1009--1026},
	Pmid = {14584564},
	Pst = {ppublish},
	Title = {Regional brain activations predicting subsequent memory success: an event-related fMRI study of the influence of encoding tasks},
	Volume = {39},
