	Journal = {NeuroImage},
	Journal-Full = {NeuroImage},
	Keywords = {recognition memory; ERP; RK},
	Mesh = {Adult; Attention; Brain; Brain Mapping; Evoked Potentials; Female; Humans; Male; Mental Recall; Psychomotor Performance; Reaction Time; Semantics},
	Number = {1},
	Pages = {406--416},
	Pmc = {PMC2175027},
	Pmid = {17933555},
