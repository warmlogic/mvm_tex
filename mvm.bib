	Number = {1},
	Pages = {65--74},
	Pmid = {12543447},
	Pst = {ppublish},
	Title = {{Paradox lost? Exploring the role of alpha oscillations during externally vs. internally directed attention and the implications for idling and inhibition hypotheses}},
	Volume = {47},
	Year = {2003},
