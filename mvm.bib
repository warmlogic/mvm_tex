	Keywords = {familiarity; faces; patients; amnesia; recognition memory},
	Mesh = {Adult; Aged; Aged, 80 and over; Amnesia; Brain; Face; Humans; Judgment; Mental Recall; Middle Aged; Neuropsychological Tests; Pattern Recognition, Visual; ROC Curve; Recognition (Psychology)},
	Number = {13},
