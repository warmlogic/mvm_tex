	Journal = {Brain Res},
	Journal-Full = {Brain research},
	Keywords = {perception; expertise; ERP},
	Mesh = {Adolescent; Adult; Brain Mapping; Cerebral Cortex; Discrimination Learning; Electroencephalography; Evoked Potentials; Humans; Memory; Neuropsychological Tests; Pattern Recognition, Visual; Photic Stimulation; Retention (Psychology); Time; Time Factors; Visual Perception},
	Pages = {204--215},
	Pmid = {18417106},
	Pst = {ppublish},
	Read = {1},
	Title = {The role of category learning in the acquisition and retention of perceptual expertise: a behavioral and neurophysiological study},
	Volume = {1210},
