	Journal = {Neuroscience and Biobehavioral Reviews},
	Journal-Full = {Neuroscience and biobehavioral reviews},
	Keywords = {ERP; source memory; recognition memory; review},
	Number = {7},
	Pages = {1066--1079},
	Pmid = {20138910},
	Pst = {ppublish},
	Rating = {0},
	Read = {1},
	Title = {Remembering perceptual features unequally bound in object and episodic tokens: Neural mechanisms and their electrophysiological correlates},
	Volume = {34},
