	Journal-Full = {Brain research},
	Keywords = {associative memory},
	Pages = {106--118},
	Pmid = {20816760},
	Pst = {ppublish},
	Rating = {5},
	Title = {Multiple ways to the prior occurrence of an event: an electrophysiological dissociation of experimental and conceptually driven familiarity in recognition memory},
	Volume = {1360},
	Year = {2010},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.brainres.2010.08.089}}

