@article{GallEtal2001,
	Abstract = {In the DRM (Deese/Roediger and McDermott) false memory paradigm, subjects studied lists of words associated with nonpresented critical words. They were tested in one of four instructional conditions. In a standard condition, subjects were not warned about the DRM Effect. In three other conditions, they were told to avoid false recognition of critical words. One group was warned before study of the lists (affecting encoding and retrieval processes), and two groups were warned after study (affecting only retrieval processes). Replicating prior work, the warning before study considerably reduced false recognition. The warning after study also reduced false recognition, but only when critical items had never been studied; when critical items were studied in half the lists so that subjects had to monitor memory for their presence or absence, the warning after study had little effect on false recognition. Because warned subjects were trying to avoid false recognition, the high levels of false recognition in the latter condition cannot be due to strategically guessing that critical test items were studied. False memories in the DRM paradigm are not caused by such liberal criterion shifts.},
	Author = {Gallo, D A and Roediger, 3rd, H L and McDermott, K B},
	Date-Added = {2010-01-14 14:38:48 -0700},
	Date-Modified = {2010-12-06 18:11:28 -0700},
