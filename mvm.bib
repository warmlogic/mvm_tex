	Mesh = {Adult; Artificial Intelligence; Brain Mapping; Female; Humans; Image Interpretation, Computer-Assisted; Magnetic Resonance Imaging; Male; Recognition (Psychology); Temporal Lobe; Young Adult},
	Number = {4},
